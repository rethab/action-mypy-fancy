{
  "name": "action-mypy-fancy",
  "version": "0.0.0",
  "description": "GitHub action that runs mypy and adds annotations to the code",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --license licenses.txt",
    "test": "jest --coverage",
    "check-all": "npm run format-check && npm run lint && npm run build && npm test",
    "all": "npm ci && npm run format && npm run lint && npm run build && npm test && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rethab/action-mypy-fancy.git"
  },
  "keywords": [
    "actions",
    "mypy"
  ],
  "author": "Reto ",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^15.14.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vercel/ncc": "^0.29.0",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.1.5",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
